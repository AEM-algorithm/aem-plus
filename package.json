    "patch": "node patch.js && cd node_modules/symbol-hd-wallets && npm install && cd ../..",
    "build": "ng build",
    "test": "ng test",
    "fix:tslint": "eslint --fix",
    "fix:prettier": "prettier --write ./src",
    "fix": "npm run fix:tslint && npm run fix:prettier",
    "e2e": "ng e2e",
    "ios:run": "sh ios.run.sh",
    "ios:rebuild:debug": "ionic cordova platform rm ios && ionic cordova platform add ios@latest --save && npm run ios:build:debug",
    "ios:build:debug": "ionic cordova build ios",
    "ios:build:prod": "ionic cordova build ios --configuration=production",
    "android:run": "ionic cordova run android -lc",
    "android:rebuild:debug": "ionic cordova platform rm android && ionic cordova platform add android@9.1.0 --save && npm run patch && npm run android:build:debug",
    "android:build:debug": "ionic cordova build android",
    "android:build:prod": "ionic cordova build android --configuration=production",
    "android:build:aab": "cd platforms/android && ./gradlew bundle && cd ../../",
    "node:space:size": "export NODE_OPTIONS=--max-old-space-size=8192",
    "env:config": "node app.config.js"
  },
